<aura:component >
	<aura:attribute name="product" type="Store_Product__c" />
    <aura:attribute name="number" type="Integer" default="5"/>
    <aura:attribute name="starColor1" type="String" default="base"/>
    <aura:attribute name="starColor2" type="String" default="base"/>
    <aura:attribute name="starColor3" type="String" default="base"/>
    <aura:attribute name="starColor4" type="String" default="base"/>
    <aura:attribute name="starColor5" type="String" default="base"/>
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
    <div class="starBackground">
    <h1 class="starh1">
        <lightning:button aura:id="star1" class="starDim" iconName="utility:favorite" title="star" variant="{!v.starColor1}"/>
        <lightning:button aura:id="star2" class="starDim" iconName="utility:favorite" title="star" variant="{!v.starColor2}"/>
        <lightning:button aura:id="star3" class="starDim" iconName="utility:favorite" title="star" variant="{!v.starColor3}"/>
        <lightning:button aura:id="star4" class="starDim" iconName="utility:favorite" title="star" variant="{!v.starColor4}"/>
        <lightning:button aura:id="star5" class="starDim" iconName="utility:favorite" title="star" variant="{!v.starColor5}"/>
    </h1>
    </div>
</aura:component>